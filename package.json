{
  "name": "auraxis-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint --max-warnings 0",
    "prepare": "husky",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "flags:check": "node scripts/check-feature-flags.cjs",
    "contracts:sync": "node scripts/contracts-sync.cjs",
    "contracts:check": "node scripts/contracts-check.cjs",
    "policy:check": "node scripts/check-frontend-governance.cjs",
    "quality-check": "npm run lint && npm run typecheck && npm run policy:check && npm run contracts:check && npm run test:coverage",
    "test:watch": "jest --watchAll",
    "ci:local": "bash scripts/run_ci_like_actions_local.sh"
  },
  "dependencies": {
    "@expo-google-fonts/playfair-display": "^0.4.2",
    "@expo-google-fonts/raleway": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.9.8",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/react-query": "^5.90.21",
    "axios": "^1.13.6",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-secure-store": "^55.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.5",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.71.2",
    "react-native": "0.81.6",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.15.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "husky": "^9.1.7",
    "jest-expo": "^54.0.17",
    "lint-staged": "^16.2.7",
    "openapi-typescript": "^7.13.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": "22.x"
  },
  "private": true,
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix --max-warnings 0 --no-warn-ignored",
      "node scripts/check-frontend-governance.cjs"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  }
}
